        "fullName": "recipient",
        "address1": "street_address",
        "city": "locality",
        "postalCode": "postal_code",
        "stateCode": "region",
        "countryCode": "country",
        "phone": "phone_number"
    },
    "consents": {
        "custom.isNewsletter": "newsletter"
    },
    "custom_fields": {
        "custom.cardNo": "loyalty_card_number"
    }
}</preference>
